<div class="container">
  <h1>Categories</h1>
  <div class="col-xs-12">
    <% @categories.each do |cate| %>
      <div class="col-xs-4 left-image-position padding-images">
        <%= link_to image_tag(cate[:image], class: "image_border "), category_path(cate[:category]), title: cate[:category].titleize %>
      </div>
    <% end %>
  </div>
</div>


